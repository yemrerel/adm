  async function guncelleStats(donem){
        $.ajax({
          url: '/getStatistic',
          method: 'get',
          data:{donem:donem}
          }).done(function(res) {              
                $("#kkSayi").text(res.istatistikler[0].KKKisiSayisi) 
                $("#gebeSayi").text(res.istatistikler[0].GebeSayisi) 
                $("#geziciSayi").text(res.istatistikler[0].GhKisiSayisi) 
                $("#bebekSayi").text(res.istatistikler[0].BebekSayisi) 

                $("#bebekGY").text(res.istatistikler[0].BebekIzlemGereken+'/'+res.istatistikler[0].BebekIzlemYapilan) 
                $("#bebekB").text('%'+(res.istatistikler[0].BebekIzlemYapilan*100)/res.istatistikler[0].BebekIzlemGereken) 

                $("#cocukGY").text(res.istatistikler[0].CocukIzlemGereken+'/'+res.istatistikler[0].CocukIzlemYapilan) 
                $("#cocukB").text('%'+(res.istatistikler[0].CocukIzlemYapilan*100)/res.istatistikler[0].CocukIzlemGereken) 

                $("#gebeGY").text(res.istatistikler[0].GebeIzlemGereken+'/'+res.istatistikler[0].GebeIzlemYapilan) 
                $("#gebeB").text('%'+(res.istatistikler[0].GebeIzlemYapilan*100)/res.istatistikler[0].GebeIzlemGereken) 
                if(res.bebekizlemRapor.length!=undefined){
                        $( ".bebekistatistikTable" ).empty()
                        for (let index = 0; index < res.bebekizlemRapor.length; index++) {
                                const h = res.bebekizlemRapor[index];
                                html='<div class="row" name="'+h.BebekKimlikNo+'" style="background-color: burlywood;border-style: groove;border-color: darkgrey;">'
                                    html+='<div class="col-md-12">'
                                        html+='<div class="row" style="margin-top: 1%;">   '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>TC:</b>'+h.BebekKimlikNo+'</div>'
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>Bebek Ad Soyad:</b>'+h.BebekAdSoyad+'</div> '
                                        html+='</div> '
                                        html+='<div class="row" style="margin-top: 1%;">   '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>İzlem Yapılan Tarih:</b>'+h.IzlemYapilanTarih+'</div> '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>Son İzlem Tarihi:</b>'+h.IzlemSonTarih+'</div>'
                                        html+='</div> '
                                    html+='</div> '
                                html+='</div> '
                                  
                                    $( ".bebekistatistikTable" ).append(html)                                         
                        }
                }
                if(res.cocukizlemRapor.length!=undefined){
                        $( ".cocukistatistikTable" ).empty()
                        for (let index = 0; index < res.cocukizlemRapor.length; index++) {
                                const h = res.cocukizlemRapor[index];
                                html='<div class="row" name="'+h.HastaTc+'" style="background-color: burlywood;border-style: groove;border-color: darkgrey;">'
                                    html+='<div class="col-md-12">'
                                        html+='<div class="row" style="margin-top: 1%;">   '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>TC:</b>'+h.HastaTc+'</div>'
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>Bebek Ad Soyad:</b>'+h.HastaAdSoyad+'</div> '
                                        html+='</div> '
                                        html+='<div class="row" style="margin-top: 1%;">   '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>İzlem Yapılan Tarih:</b>'+h.IzlemYapilanTarih+'</div> '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>Son İzlem Tarihi:</b>'+h.IzlemSonTarih+'</div>'
                                        html+='</div> '
                                    html+='</div> '
                                html+='</div> '
                                  
                                    $( ".cocukistatistikTable" ).append(html)                                         
                        }
                }
                if(res.gebeizlemRapor.length!=undefined){
                        $( ".gebeistatistikTable" ).empty()
                        for (let index = 0; index < res.gebeizlemRapor.length; index++) {
                                const h = res.gebeizlemRapor[index];
                                html='<div class="row" name="'+h.GebeKimlikNo+'" style="background-color: burlywood;border-style: groove;border-color: darkgrey;">'
                                    html+='<div class="col-md-12">'
                                        html+='<div class="row" style="margin-top: 1%;">   '
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>TC:</b>'+h.GebeKimlikNo+'</div>'
                                               html+='<div class="col-md-6" style="font-size: smaller;color: 325179;"><b>Gebe Ad Soyad:</b>'+h.GebeAdSoyad+'</div> '
                                        html+='</div> '
                                        html+='<div class="row" style="margin-top: 1%;">   '
