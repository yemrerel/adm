$(document).ready( function() {
    var now = new Date();
    var month = (now.getMonth() + 1);               
    var day = now.getDate();
    if (month < 10) 
        month = "0" + month;
    if (day < 10) 
        day = "0" + day;
    var today = now.getFullYear() + '-' + month + '-' + day;
    $('#bittar').val(today);
    $('#bastar').val(today);
    $('#istar').val(today);
    $('#kantar').val(today);

});
</script>


