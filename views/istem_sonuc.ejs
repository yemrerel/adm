$( "#kayitEkleForm" ).on( "submit", function( event ) {
     event.preventDefault();
     var input = $("<input>").attr("type", "hidden").attr("name", "hastaTC").attr("id", "hastaTC").val(<%=hasta.Id%>);
     $('#kayitEkleForm').append(input);
     var input = $("<input>").attr("type", "hidden").attr("name", "secilenTanılar").attr("id", "secilenTanılar").val(JSON.stringify(secilenTanılar));
     $('#kayitEkleForm').append(input);
     var input = $("<input>").attr("type", "hidden").attr("name", "secilenTestler").attr("id", "secilenTestler").val(JSON.stringify(secilenTestler));
     $('#kayitEkleForm').append(input);
      $.ajax({
           type: "get",
           url: '/alis/ALISKayitEkleAdv',
           data:$( this ).serialize(),
           success: function(data)
           {
               if(data.Res==1){
                  alert('Kayıt Başarıyla Eklenmiştir.')
               }
               else {alert(data.HataMesaji)}
               $("#hastaTC").remove()
               $("#secilenTanılar").remove()
               $("#secilenTestler").remove()
           }
       });
});

