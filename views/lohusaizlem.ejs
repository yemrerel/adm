                         <div  class="row"style="background-color: blue;color: white;padding: 2%;margin: auto; width: 92%;margin-top:2%;font-size: 0.8rem;">  
                               İzlem 3:<p style="margin: 0;"> <%=iz[i].Tarih%> </p>
                         </div>
                         <%break } %>                          
             <%} %>                                                      
        </div>
        <div id="b4" class="" style="width: 20%;">  
            <%  for(var i=0; i < iz.length ; i++) {%>
                    <%if (iz[i].KacinciIzlem==4) { %>
                         <div  class="row"style="background-color: blue;color: white;padding: 2%;margin: auto; width: 92%;margin-top:2%;font-size: 0.8rem;">  
                               İzlem 1:<p style="margin: 0;"> <%=iz[i].Tarih%> </p>
                         </div>
                   <%break } %>                          
             <%} %>                                                      
        </div>
        <div id="b5" class="" style="width: 20%;">  
           <% k=0
           for(var i=0; i < iz.length ; i++) {%>
                   <%if (iz[i].KacinciIzlem==99 && k<4) { 
k++    %>
