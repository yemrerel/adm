    <div class="row" style="margin-top:1%;height: 80%;background-color:white;border-radius: 15px;border-bottom-left-radius: 0;border-bottom-right-radius: 0;">
        <div id="b1" class="" style="width: 20%;">  
           <% for(var i=0; i < iz.length ; i++) {%>
                   <%if (iz[i].KacinciIzlem==1) { %>
                         <div  class="row"style="background-color: blue;color: white;padding: 2%;margin: auto; width: 92%;margin-top:2%;font-size: 0.8rem;">  
