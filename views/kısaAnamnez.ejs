$( "#mynBitirBtn").click(function() {

    var input = $("<input>").attr("type", "hidden").attr("name", "secilenTanilar").val(JSON.stringify(secilenTanılar));
    $('#tanilariKaydetForm').append(input);
    var input = $("<input>").attr("type", "hidden").attr("name", "MESLEKI_MARUZIYET").val(JSON.stringify(MESLEKI_MARUZIYET));
    $('#tanilariKaydetForm').append(input);
