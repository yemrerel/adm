<script>

raporlar=[]


$( "#ilacRaporBtn").click(function() {
      $.ajax({
            url: '/ilacRaporlariniGetir',
            method: 'post',
            data:{tc:<%= hasta.Id%>}
            }).done(function(res) {
                    if(res!=null){
                        $("#RaporTable").empty()
                        etkinMaddeler=[]
                        for (let index = 0; index < res.length; index++) {//eraporEtkinMaddeListesi
                            const e = res[index];
                            console.log(e)
                            html= `<div class="row"> 
                                        <div class="col-md-12">
                                                 <div class="row"> 
                                                        <div class="col-md-6" style="background-color: white;text-align: center;padding: 2px;font-size: smaller;"> 
                                                                    <u style="color: #325179;font-weight: bold;">Rapor Takip No:</u>`+e.raporTakipNo+`
                                                        </div>
                                                        <div class="col-md-6" style="background-color: white;text-align: center;padding: 2px;font-size: smaller;"> 
                                                                    <u style="color: #325179;font-weight: bold;">Rapor Tarihi:</u>`+e.raporTarihi+`
                                                        </div>
                                                  </div>`
                                                   if(e.eraporEtkinMaddeListesi.length==undefined){//array değil tek eleman
                                                             etkinMaddeler.push(e.eraporEtkinMaddeListesi)
                                                    }
                                                    else{
                                                        for (let i = 0; i < e.eraporEtkinMaddeListesi.length; i++) {
                                                            etkinMaddeler.push(e.eraporEtkinMaddeListesi[i])
                                                        }
                                                    }
                                                    for (let k = 0; k < etkinMaddeler.length; k++) {
                                                        html+=`<div class="row">
                                                                    <div class="col-md-4" style="font-size: smaller;background-color: white;text-align: center;padding: 2px;">
                                                                        <u style="color: #325179;font-weight: bold;">Etkin Madde Kodu:</u> `+etkinMaddeler[k].etkinMaddeKodu+`
                                                                    </div>
                                                                    <div class="col-md-4" style="font-size: smaller;background-color: white;text-align: center;padding: 2px;">
                                                                                <u style="color: #325179;font-weight: bold;">Etkin Madde Adı:</u> `+etkinMaddeler[k].etkinMaddeDVO.adi+` 
                                                                   </div>
                                                                   <div class="col-md-4" style="font-size: smaller;background-color: white;text-align: center;padding: 2px;">
                                                                                <u style="color: #325179;font-weight: bold;">Etkin Madde Adı:</u> `+etkinMaddeler[k].etkinMaddeDVO.formu+` 
                                                                   </div>
                                                                </div> `                                                   
                                                    }

                                           html+=` <div class="row" style="border-bottom: solid;"> 
                                                       <div class="col-md-12" style="font-size: smaller;background-color: white;text-align: center;padding: 2px;">
                                                            <u style="color: #325179;font-weight: bold;">Rapor Açıklaması:</u>`+e.eraporAciklamaListesi.aciklama+` 
                                                        </div>                              
                                            </div></div>`
                                    $("#RaporTable").append(html);
                        }            

                        $('#raporIlacModal').modal('show');
                    }
                   
            });   
});



