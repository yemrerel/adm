    $('#tanilariKaydetForm').append(input);
    var input = $("<input>").attr("type", "hidden").attr("name", "hastaId").val((<%=hasta.Id%>));
    $('#tanilariKaydetForm').append(input);

    $.ajax({
        url: '/hasta/kisaAnamnez/TaniKaydet',
        method: 'post',
        data: $("#tanilariKaydetForm").serialize()
        }).done(function(res) {
            if(res!=null){
                alert('Muayene Kaydedilmiştir.')
                var input = $("<input>").attr("type", "hidden").attr("name", "isim").val(<%=hasta.Id%>);
                            $('#hastaProfili').append(input); 
                            $("#hastaProfili").submit();      
            }
    });
});
