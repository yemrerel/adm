<div class="container">     
    <div class="row" style="margin-top:2%">          
        <div class="input-group input-group-sm col-md-4">
                <div class="input-group-prepend " style="margin-left: 1px;">
                        <span class="input-group-text " id="inputGroup-sizing-default">Stok</span>
                </div>
                <select name="stoklar" id="stoklar" class="browser-default custom-select"> 
                      <% stoklar.forEach(function(e) { %>
                        <option value="<%=e.ID %>"> <%=e.Ad %></option>
                        <%}); %>                     
                </select>
        </div>
         <div class="col-md-6" style="margin:auto">
               İşlem Tarihi <input type="date">ile <input type="date"> arasındakiler
         </div>
         <div class="col-md-2" style="margin:auto">
           <button type="button">Analiz Et</button>
        </div>
    </div>
    
</div>


<div class="container">     
    <div class="row" style="margin-top:1%;">   
            <div class="col-md-6" style="margin:auto;text-align: center ;">
                <div class="row" style="margin-top:1%">   
                     <div class="" style="text-align: center; margin: auto;"><b> <u>Yeni Kullanıcı</u> </b></div>
                </div>
                <div class="row">   
                        <div class="container" style="margin-top: 1%; background-color: #adb5bd;text-align: center;border-bottom-left-radius: 0;border-bottom-right-radius: 0;border-top-left-radius: 15px;border-top-right-radius: 15px;overflow-y: auto;overflow-x: hidden;width: 98%;">
                            <div class="row">
                                
                                <div class="col-md-3" style="text-align: center;padding: 1px;border-right: solid;border-bottom: solid;font-size: smaller;border-width: thin;">
                                    Tc
                                </div>  
                                <div class="col-md-3" style="padding: 1px;text-align: center;border-bottom: solid;font-size: smaller;border-width: thin;border-right: solid;border-width: thin;">
                                    Ad Soyad
                                </div>
                                <div class="col-md-3" style="padding: 1px;text-align: center;border-bottom: solid;font-size: smaller;border-right: solid;border-width: thin;">
                                    Baba Ad
                                </div>                                      
                                <div class="col-md-3" style="border-top-right-radius: 15px;padding: 1px;text-align: center;border-bottom: solid;font-size: smaller;border-width: thin;">
                                    Doğum Tarih
                                </div>
                            </div>
                        </div>
                        <div class="container" id="hastalarTable" style="background-color: white; text-align: center;box-shadow: 6px 7px 6px grey;height: 12rem;border-radius: 20px;overflow-y: overlay;overflow-x: hidden;border-top-left-radius: 0;border-top-right-radius: 0;width: 98%;">
                            <% stokGirisleri.forEach(function(e) { %>
                            <div  class="row stokRow" id="<%='s'+e.ID %>" >
                                <div class="col-md-3 kod c1" style="text-align: center;padding: 5px;border-right: solid;border-bottom: solid;border-width: thin;font-size: smaller;"><%=e.Tarih %> </div>  
                                <div class="col-md-3 barkod c2" style="text-align: center;padding: 5px;border-right: solid;border-bottom: solid;border-width: thin;font-size: smaller;"><%=e.Miktar %></div>  
                                <div class="col-md-3 c3" style="padding: 1px;text-align: center;border-bottom: solid;border-right: solid;border-width: thin;font-size: smaller;"><%=e.Miad %></div>
                                <div class="col-md-3 c10" style="padding: 1px;text-align: center;border-bottom: solid;border-width: thin;font-size: smaller;"><%=e.Aciklama %></div>
                            </div>
                            <%}); %>  

                        </div>

                        <div class="container">          
                            <div class="row" style="margin-top:2%;">
                                    <div class="col-md-4">
                                                    <button id="yeniBtn" type="button" class="btn btn-block btn-lg" style="background-color: #325179;color: white;border-radius: 8px;font-size: 1rem;padding: 3px;">Yeni</button>
                                    </div>
                                    <div class="col-md-4">
                                                    <button id="duzenleBtn" type="button" class="btn btn-block btn-lg" style="background-color: #325179;color: white;border-radius: 8px;font-size: 1rem;padding: 3px;">Düzenle</button>
                                </div>
                                    <div class="col-md-4">
                                                <button id="silBtn" type="button" class="btn btn-block btn-lg" style="background-color: #325179;color: white;border-radius: 8px;font-size: 1rem;padding: 3px;">Sil</button>
                                </div>                                      
                            </div>
                        </div>
                    </div>
           </div>
           <div class="col-md-6" style="margin:auto;text-align: center ;">
                <div class="row" style="margin-top:1%">   
