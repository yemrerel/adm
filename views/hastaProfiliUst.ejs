<script>


  $('.izlemlerROW').click(function() { 
        $('.izlemlerROW').css('background-color','');
        $(this).css('background-color','aqua');
        izlemId=$(this).attr('id').substring(5)
        izlemBilgi='izlemBilgileri'+izlemId
        $('.izlemBilgileriROW').hide()
        $('#'+izlemBilgi).show(); 
  });
  
  
  
  secilenHgler=[]
  
  <% hasta_hastaGrupları.forEach(function(e) { %>
    secilenHgler.push({ADI:'<%=e.ADI%>',grupId:<%=e.grupId%>});
  <%}); %>
  
  
  $('#kaydetHGBtn').click(function() { 
    $.ajax({
              url: '/hastaGruplari',
              method: 'get',
              data: { secilenHgler: secilenHgler,tc:<%=hasta.Id%> }
              }).done(function(res) {
                 
      });
  });
  $('#yeniGrupBtn').click(function() { 
      yeniAd=$('#yeniGrupAdi').val()
      $.ajax({
              url: '/addgroup',
              method: 'get',
              data: {yeniAd:yeniAd}
              }).done(function(res) {
                if(res.result){
                      html='<div class="row hgRow"> <div class="col-md-3 cbx" style="background-color: white;text-align: right;padding: 4px;border-bottom: solid;">'
                      html+='<div class="tik" style="background-color:red;height: 25px;width: 25px;margin-left: 46%;" name="'+res.Id+'"></div>'
                      html+=' </div>  <div class="col-md-9 isim" style="background-color: white;padding: 4px;text-align: center;border-bottom: solid;">'+$('#yeniGrupAdi').val()+'</div> </div>'
                      $("#hgTable").append(html);
                }
                
                
        });
  });
  $('#editProfile').click(function() { 
    $('.uneditableBody').css('display','none')
    $('.editableBody').css('display','')
  });
  $(document).on('click', "#kaydetDF", function(){
    $('#dfForm').append('<input type="hidden" name="hastaTc" value="<%=hasta.Id%>" />');
  
   
    $.ajax({
              url: '/kaydetDF',
              type: 'post',
              data: $('#dfForm').serialize()
              }).done(function(res) {
                $('.uneditableBody').css('display','')
                $('.editableBody').css('display','none')  
      });
      
  });
  
  
    $(document).on('click', ".hgRow", function(){
  
                if($(this).children(".cbx").children().css('background-color')=="rgb(255, 0, 0)"){//kırmızı mı
                    $(this).children(".cbx").children().css('background-color','rgb(0, 255, 0)');//yeşil
                    var grupId= $(this).children(".cbx").children().attr("name");
                    var ADI= $(this).children(".isim").html();
                    
                    html=' <div id="'+grupId+'" class="row hhgRow">'
                   html+='<div class="col-md-12 isim" style="background-color: white;padding: 4px;text-align: center;border-bottom: solid;">'+ADI+'</div>     </div>'
  
                    $("#secilenHgler").append(html);
                    secilenHgler.push({ADI:ADI,grupId:grupId});
  
                }
                else{
                    $(this).children(".cbx").children().css('background-color','red')
                    var grupId= $(this).children(".cbx").children().attr("name").split("?")[0];
                    $('#'+grupId).remove()
                    secilenHgler.forEach(element => {
                        if(element.grupId==grupId){
                          secilenHgler = jQuery.grep(secilenHgler, function(value) {
                                return value != element;
                              });
            
                        }
                    });
                }
  });
  
  
  
  
  
    
    </script>
  
  

