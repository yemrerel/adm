<script>

  $( ".sigaraBtn" ).click(function() {
     $( "#sigaraModal" ).modal('toggle')
  });
  $( "#sigaraKaydetBtn" ).click(function() {
    sigDurum=0,sigAdet=0,sbGonder=0
    if($("#sigDurum").is(":checked")) sigDurum=1            
    sigAdet=$("#sigAdet").val()
    if($("#sbGonder1").is(":checked")) sbGonder=1            

             $.ajax({
                type: "post",
                url: '/hasta/sigaraGuncelle',
                data:{sigDurum:sigDurum,sigAdet:sigAdet,sbGonder:sbGonder},
                success: function(data)
                {
                    if(data) {
                          alert('Durum güncellenmiştir.')
                          if(sigDurum==0){
                             $('#sigaraImg').attr("src", "/public/img/noSmoke.svg");
                             $('#sigaraDurum').html('Kullanmıyor')
                          }
                          else{
                             $('#sigaraImg').attr("src", "/public/img/smoke.svg");
                             $('#sigaraDurum').html('Kullanıyor')
                          }                      
                          $( "#sigaraModal" ).modal('toggle')      
                    }                       
                }
            });
  });


</script>



