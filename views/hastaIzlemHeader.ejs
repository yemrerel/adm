  function ilceleriYukle(ilkodu) {if(ilkodu!=undefined){
                    $('#ilceler').empty();
                    $.ajax({
                          url: '/ilceleriGetir',
                          method: 'get',
                          data: { ilkodu: ilkodu }
                    }).done(function(res) {
                           var ilceArray=res.ilceler;
                           for (var i = 0; i <= ilceArray.length; i++) {
                               
                                  $('#ilceler').append('<option value="' + ilceArray[i].KODU + '">' + ilceArray[i].ADI + '</option>');
                            }
                    });
                  $('#islemYer').empty();
                  
               }
        
     };
     function kurumlarıYukle(ilcekodu) {if(ilcekodu!=undefined){
           $('#islemYer').empty();
           $.ajax({
            url: '/kurumlariGetir',
            method: 'get',
            data: { ilcekodu: ilcekodu }
                }).done(function(res) {
                  var kurumArray=res.kurumlar;
                  for (var i = 0; i <= kurumArray.length; i++) {                  
                    $('#islemYer').append('<option value="' + kurumArray[i].KODU + '">' + kurumArray[i].ADI + '</option>');
                  }
                });
          }
     };

  
  </script>





